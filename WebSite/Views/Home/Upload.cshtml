@{
    ViewBag.Title = "Upload kitteh";
}

<h2>@ViewBag.Title.</h2>
<br/>

@using (Html.BeginForm("Upload",
                        "Home",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{
    <label for="file">Chooz image file:</label>
    <input type="file" name="file" id="file" /><br /><br />
    <input type="submit" value="Upload kitteh's soul" class="btn btn-primary" />
    <br /><br />
    @ViewBag.Message
}